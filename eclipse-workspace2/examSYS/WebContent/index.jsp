					<input type="password" class="form-control"  placeholder="your passwoed" name="pwd">
				</div>
				<button type="submit" class="btn btn-primary mb-2">ddddd</button>
			</form>
		</div>
		<div class="col-md-6 my-md-auto pb-5 pb-md-0 order-1 order-md-2">
			<img alt="Mockup" class="img-fluid d-block mx-auto" src="assets/images/mockup.svg"/>
		</div>
	</div>
</div>
</section>

<!--Features Section-->
<section class="py-5">
<div class="container">
	<div class="row feature-grid">
		<div class="col-md-4 col-sm-6">
			<div class="media">
				<div class="icon-box">
					<div class="icon-box-inner">
						<span data-feather="check-circle" width="28" height="28"></span>
					</div>
				</div>
				</div>
				</div>
				</div>
				</div>
				</section>

<!-- jQuery first, then Popper.js, then Bootstrap JS -->
<script src="assets/js/jquery-3.2.1.min.js"></script>
<script src="assets/js/feather.min.js"></script>
<script src="assets/js/scripts.js"></script>
